@model IEnumerable<AspNetCore_MVC_Project.Models.Job>

@{
    ViewData["Title"] = "Business Data";
}

<h2>Business Data</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Analog Value</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.AnalogValue</td>
            </tr>
        }
    </tbody>
</table>

