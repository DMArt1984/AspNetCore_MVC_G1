@model IEnumerable<AspNetCore_MVC_Project.Models.Statistic>

@{
    ViewData["Title"] = "KPI Statistics";
}

<h2>KPI Statistics</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Kf1</th>
            <th>Kf2</th>
            <th>Kf3</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.Kf1</td>
                <td>@item.Kf2</td>
                <td>@item.Kf3</td>
            </tr>
        }
    </tbody>
</table>

