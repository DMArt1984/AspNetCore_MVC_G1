<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - AspNetCore_MVC_Project BOX</title>

    <!-- Подключение Bootstrap 5 для стилизации -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" />
</head>
<body>
    <header>
        
    </header>

    <!-- Основной контейнер, в котором рендерится контент страниц -->
    <div class="container mt-4">
        @RenderBody()
    </div>

    <!-- Футер (подвал страницы) -->
    <footer class="border-top footer text-muted text-center mt-4">
        <div class="container">
            &copy; 2024 - AspNetCore_MVC_Project ABC
        </div>
    </footer>

    <!-- Подключение скриптов Bootstrap 5 -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>


